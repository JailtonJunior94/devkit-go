{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/httpserver/main.go"
        },
        {
            "name": "[Start] Order API",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/order/main.go",
            "args": [
                "api"
            ]
        },
        {
            "name": "[Start] Consumer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/order/main.go",
            "args": [
                "consumers"
            ]
        },
    ]
}